{

  "manifest_version": 2,
  "name": "BOM Data",
  "short_name": "BOM Data",
  "version": "1.0.7",
  "homepage_url": "http://tamef016.amer.int.tenneco.com/cae/#Tools",
  "description": "Displays an address bar button with menu to list BOM data of the opened print.",
  "icons": {
    "48": "images/Monkey.svg",
    "96": "images/Monkey.svg",
    "128": "images/Monkey.svg"
  },
  
  "background": {
    "scripts": ["js/background.js"]
  },
  
  "applications": {
    "gecko": {
      "id": "BOMData@tenneco.com",
      "strict_min_version": "62.0",
      "update_url": "https://s3.us-east-2.amazonaws.com/drivmilan/BOMData.json"
    }
  },
    
  "permissions": [
    "theme",
    "tabs",
    "http://pcdcforaxap01/*"
  ],  
  
  "page_action": {
    "browser_style": true,
    "show_matches": ["http://170.64.172.81/CLEVPRNT/*.pdf","http://millap01.na.ten/CLEVPRNT/*.pdf","http://170.64.172.81/clevprnt/*.pdf","http://tamef015vm26.amer.int.tenneco.com/CLEVPRNT/*.pdf","http://tamef015vm26.amer.int.tenneco.com/clevprnt/*.pdf","http://tamef015vm26.amer.int.tenneco.com/CLEVPRNT/*.PDF"],
    "default_icon": {
      "19": "images/Monkey.svg",
      "38": "images/Monkey.svg"
    },
    "default_title": "BOM Data"
   },
   
   "sidebar_action": {
    "default_icon": {
      "16": "images/Monkey.svg",
      "32": "images/Monkey.svg"
   },
    "default_title": "BOM Data",
    "default_panel": "sidebar/panel.html",
    "open_at_install": true
   }

}